{
  "name": "behance-api",
  "version": "1.1.4",
  "description": "Node.js wrapper for the Behance API",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "lint": "tslint --config tslint.json --project .",
    "test": "mocha --reporter spec",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polyneue/behance-api.git"
  },
  "keywords": [
    "behance",
    "api",
    "wrapper"
  ],
  "author": "Ed Mendoza <polyneue@gmail.com> (http://www.edmendoza.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Polyneue/behance-api/issues"
  },
  "homepage": "https://github.com/Polyneue/behance-api#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "qs": "^6.3.0",
    "request": "^2.76.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "chai": "^4.0.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "nock": "^9.0.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.0.5"
  }
}
